{
    "name": "xadminx/limb",
    "description": "Limb4 php framework",
    "license": "MIT",
    "type": "project",
    "require": {
        "php" : ">=7.2"
    },
    "require-dev": {
    },
    "replace": {
        "limb/acl": "self.version",
        "limb/active_record": "self.version",
        "limb/cache": "self.version",
        "limb/cache2": "self.version",
        "limb/calendar": "self.version",
        "limb/cli": "self.version",
        "limb/cms": "self.version",
        "limb/config": "self.version",
        "limb/core": "self.version",
        "limb/cron": "self.version",
        "limb/datetime": "self.version",
        "limb/dbal": "self.version",
        "limb/filter_chain": "self.version",
        "limb/fs": "self.version",
        "limb/i18n": "self.version",
        "limb/imagekit": "self.version",
        "limb/log": "self.version",
        "limb/macro": "self.version",
        "limb/mail": "self.version",
        "limb/net": "self.version",
        "limb/optimization": "self.version",
        "limb/search": "self.version",
        "limb/session": "self.version",
        "limb/taskman": "self.version",
        "limb/tests_runner": "self.version",
        "limb/toolkit": "self.version",
        "limb/tree": "self.version",
        "limb/twig": "self.version",
        "limb/validation": "self.version",
        "limb/view": "self.version",
        "limb/web_agent": "self.version",
        "limb/web_app": "self.version",
        "limb/web_cache": "self.version",
        "limb/web_spider": "self.version",
        "limb/wysiwyg": "self.version"
    },
    "autoload": {
        "files": [
            "src/limb/bootstrap.php"
        ],
        "psr-0": {
            "limb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0" : {
            "limb\\tests\\" : "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

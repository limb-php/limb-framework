{
    "name": "xadminx/limb",
    "description": "Limb4 php framework",
    "version": "4.0.0-dev",
    "type": "project",
    "require": {
        "php" : ">=7.2"
    },
    "replace": {
        "limb/auth": "self.version"
    },
    "autoload": {
        "files": [
            'limb/acl/common.inc.php',
            'limb/active_record/common.inc.php',
            'limb/cache/common.inc.php',
            'limb/cache2/common.inc.php',
            'limb/core/common.inc.php'
        ],
        "psr-4": {
            "limb\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
